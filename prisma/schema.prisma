generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // directUrl = env("DATABASE_URL_UNPOOLED")
}

model User {
  id           String @id @unique
  email        String
  firstName    String
  lasName      String
  profileImage String

  createdAt DateTime @default(now())
}
